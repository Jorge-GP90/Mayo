<div class="col-md-1">
 </div>
 <div class="col-xs-12 col-md-11">
   <h1 class="mb-3"><%= 'New label' %></h1>
   <div class="error_unit">
     <% if @label.errors.any? %>
       <div id="error_explanation">
         <h2><%= pluralize(@label.errors.count, "error") %> prohibited this label from being saved:</h2>
         <ul>
         <% @label.errors.full_messages.each do |msg| %>
           <li><%= msg %></li>
         <% end %>
         </ul>
       </div>
     <% end %>
   </div>
   <div class="row mt-4 mb-4">
     <div class="col-xs-12 col-md-6">
       <%= form_with model: @label, local: true do |f| %>
         <div class="form-group task_unit">
           <%= f.label :label_name %>
           <%= f.text_field :label_name, class: "form-control" %>
         </div>
         <div class="form-group">
           <%= f.submit "Register", class: "btn btn-info" %>
         </div>
       <% end %>
     </div>
   </div>
   <div class="btn btn-primary new-btn  btn-sm mb-4">
   <%= link_to 'index', tasks_path %>
   </div>
 </div>